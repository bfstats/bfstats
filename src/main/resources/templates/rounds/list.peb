{% extends "layout" %}
{% block title %} Rounds List {% endblock %}

{% block content %}
{% if rounds is empty %}
There are no rounds.
{% else %}
<ul>
  {% for round in rounds %}
  <li>{{ round.id }} <a href="/rounds/{{ round.id }}">{{ round.mapName }}</a> {{ round.startTime | dt("yyyy-MM-dd HH:mm") }} - {{ round.endTime | dt("yyyy-MM-dd HH:mm") }} ({{ round.durationInMinutes }} min)</li>
  {% endfor %}
</ul>
{% endif %}

{% endblock %}