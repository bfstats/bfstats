<table class="styled_table">
  <thead>
  <tr>
    <th>#</th>
    <th>Kit</th>
    <th>Times used</th>
    <th>%</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  {% for kit in kits %}
  <tr>
    <td>{{ loop.index + 1 }}</td>
    <td>{{ kit.name }}</td>
    <td>{{ kit.timesUsed }}</td>
    <td>{{ kit.percentage }}</td>
    <td>
      <div class="meter">
        <span style="width: {{ kit.percentage }}%"></span>
      </div>
    </td>
  </tr>
  {% endfor %}
  </tbody>
</table>