{% extends "layout" %}

{% block title %} Players List {% endblock %}

{% block content %}


{% if players is empty %}
  There are no players.
{% else %}
  <ul>
    {% for player in players %}
    <li><a href="/players/{{ player.id }}">{{ player.name }}</a></li>
    {% endfor %}
  </ul>
{% endif %}

{% endblock %}